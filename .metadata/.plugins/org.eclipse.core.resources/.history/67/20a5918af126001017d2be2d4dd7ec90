package com.meghana;

class Outer {
    private int x = 10;
    class Inner {
        public void display() {
            System.out.println(x);
        }
    }
    public static void main(String[] args) {
        Outer o = new Outer();
        Outer.Inner i = o.new Inner();
        i.display();
    }
}
