package com.meghana;

public class Test {
	public static void main(String[] args) {
		String s = null;

		try {
			int length = s.length();
			System.exit(0);
		} catch (NullPointerException e) {
			System.out.println("Null Pointer Exception caught");
		} finally {
			System.out.println("Finally block executed");
		}

	}
}